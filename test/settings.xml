<settings xmlns="wsp:settings">
    <version>3.0</version>
    <sensors>
        <wsensor>
            <id>1</id>
            <name>Backbed</name>
            <readings>
                <id>1.0</id>
                <name>Temp</name>
                <position>topleft</position>
            </readings>
            <readings>
                <id>1.1</id>
                <name>Hum</name>
                <position>topright</position>
            </readings>
        </wsensor>
        <wsensor>
            <id>2</id>
            <name>Shed</name>
            <readings>
                <id>2.0</id>
                <name>Temp</name>
                <position>topleft</position>
            </readings>
            <readings>
                <id>2.1</id>
                <name>Hum</name>
                <position>middleright</position>
            </readings>
        </wsensor>
    </sensors>
    <database>
        <host>172.16.20.5</host>
        <port>5432</port>
        <database-name>test_weatherdb</database-name>
        <user>test_weather</user>
        <password>testtest</password>
    </database>
    <gpio>
        <gpio1>17</gpio1>
        <gpio2>27</gpio2>
        <gpio3>22</gpio3>
        <gpio4>18</gpio4>
        <gpio5>17</gpio5>
    </gpio>
    <i2c>
        <busno>3</busno>
        <atmega>4</atmega>
        <lcd>39</lcd>
    </i2c>
    <logging>
        <config-file>conf/logging.conf</config-file>
    </logging>
</settings>
